# Random string generator using native Danfe functions
import "./v_op/some_v_code" as v_mods

chars = 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789'
total_chars = len(chars)

# Generate random ASCII string using built-in rand_int
function gen_random_ascii(length){
    result = ''
    i = 0
    for i <= length {
        result = result + chars[int(rand_int() % total_chars)]
        i = i + 1
    }
    return result
}
